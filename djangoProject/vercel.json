{
  "builds": [
    {
      "src": "djangoProject/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)", 
      "dest": "/static/$1"   
    },
    {
      "src": "/(.*)",
      "dest": "djangoProject/wsgi.py"
    }
  ],
  "buildCommand": "python manage.py collectstatic "
}